<?xml version="1.0"?>
<!DOCTYPE tpconfig SYSTEM "http://thousandparsec.net/tp/downloads/tpconfig.dtd">
<tpconfig type="installed" xml:lang="en">
    <aiclient name="daneel-ai">
        <longname xml:lang="en">daneel-ai</longname>
        <version>0</version>
        <description xml:lang="en">An advanced rule based AI.</description>
        <commandstring>daneel-ai</commandstring>
        <cwd>PREFIX/bin</cwd>
        <forced>-f %(rname)s</forced>
        <forced>-u tp://%(user)s:password@localhost:%(port)d</forced>
        <rules>risk</rules>
    </aiclient>
</tpconfig>
