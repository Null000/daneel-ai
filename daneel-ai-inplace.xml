<?xml version="1.0"?>
<!DOCTYPE tpconfig SYSTEM "http://thousandparsec.net/tp/downloads/tpconfig.dtd">
<tpconfig type="inplace" xml:lang="en">
    <aiclient name="daneel-ai-inplace">
        <longname xml:lang="en">daneel-ai (Development)</longname>
        <version>0</version>
        <description xml:lang="en">An advanced rule based AI.</description>
        <commandstring>daneel-ai.py</commandstring>
        <cwd>.</cwd>
        <forced>-f %(rname)s</forced>
        <forced>-u tp://%(user)s:password@localhost:%(port)d</forced>
        <rules>risk</rules>
        <rules>rfts</rules>
        <rules>minisec</rules>
        <rules>mtsec</rules>
    </aiclient>
</tpconfig>
